{
  "rust-analyzer": {
    "server": {
      "path": "/bin/rust-analyzer"
    },
    "procMacro": {
      "ignored": {"async_trait": ["async_trait"]}
    }
  },
  "languageserver": {
    "ocaml": {
      "command": "ocamllsp",
      "filetypes": ["ocaml", "reason"]
    },
    "metals": {
      "command": "metals-vim",
      "rootPatterns": ["build.sbt"],
      "filetypes": ["scala", "sbt"]
    }
  },
  "typescript": {
    "preferences": {
      "importModuleSpecifier": "non-relative"
    }
  },
  "emmet": {
    "includeLanguages": ["javascriptreact"]
  }
}
